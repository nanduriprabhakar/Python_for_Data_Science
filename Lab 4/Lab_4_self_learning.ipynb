    "def factorial(a):     ## Using recursion to work with factorial. \n",
    "    result = 1;\n",
    "    if (a==0):\n",
    "        return result;\n",
    "    else:\n",
    "        result = a * factorial(a-1);  \n",
    "        return result;\n",
    "\n",
    "print(factorial(35));"
   ]
